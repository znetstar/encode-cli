{
    "name": "encode-cli",
    "description": "CLI for @znetstar/encode-tools-native",
    "version": "1.4.0",
    "author": "Zachary Boyd <zachary@zacharyboyd.nyc>",
    "bin": {
        "encli": "./bin/run",
        "encode-cli": "./bin/run"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "bugs": "https://github.com/znetstar/encode-cli/issues",
    "dependencies": {
        "@oclif/command": "^1.8.0",
        "@oclif/config": "^1.17.0",
        "@oclif/plugin-help": "^3.2.2",
        "@oclif/plugin-plugins": "^1.10.1",
        "@znetstar/encode-tools-native": "^1.0.1",
        "tslib": "^1.14.1"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1.26.0",
        "@oclif/test": "^1.2.8",
        "@types/chai": "^4.2.21",
        "@types/mocha": "^5.2.7",
        "@types/node": "^10.17.60",
        "chai": "^4.3.4",
        "globby": "^10.0.2",
        "mocha": "^5.2.0",
        "nyc": "^14.1.1",
        "ts-node": "^8.10.2",
        "typescript": "^4.2.x"
    },
    "engines": {
        "node": ">=12.22.6"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/znetstar/encode-cli",
    "keywords": [
        "oclif"
    ],
    "license": "LGPL-3.0",
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "encli",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins"
        ]
    },
    "repository": "znetstar/encode-cli",
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
        "test": "true",
        "version": "oclif-dev readme && git add README.md"
    },
    "types": "lib/index.d.ts"
}
